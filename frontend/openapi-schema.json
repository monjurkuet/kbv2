    "/api/v1/documents/{document_id}/spans": {
      "get": {
        "tags": [
          "documents"
        ],
        "summary": "Get Document Spans",
        "description": "GET /api/v1/documents/{document_id}/spans\n\nGet all text spans with entity references for exact text highlighting.\nCalculates absolute character offsets using the OffsetCalculationService.\n\nCritical for \"Click entity -> See text evidence\" functionality.\nImplements W3C Web Annotation Data Model compliance.",
        "operationId": "get_document_spans_api_v1_documents__document_id__spans_get",
        "parameters": [
          {
            "name": "document_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid",
              "description": "Document UUID",
              "title": "Document Id"
            },
            "description": "Document UUID"
          },
          {
            "name": "confidence_threshold",
            "in": "query",
            "required": false,
            "schema": {
              "type": "number",
              "maximum": 1.0,
              "minimum": 0.0,
              "description": "Minimum confidence threshold",
              "default": 0.0,
              "title": "Confidence Threshold"
            },
            "description": "Minimum confidence threshold"
          },
          {
            "name": "entity_types",
            "in": "query",
            "required": false,
            "schema": {
              "anyOf": [
                {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                },
                {
                  "type": "null"
                }
              ],
              "description": "Filter by entity types",
              "title": "Entity Types"
            },
            "description": "Filter by entity types"
          },
          {
            "name": "verified_only",
            "in": "query",
            "required": false,
            "schema": {
              "type": "boolean",
              "description": "Only return spans with grounding quotes",
              "default": false,
              "title": "Verified Only"
            },
            "description": "Only return spans with grounding quotes"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
